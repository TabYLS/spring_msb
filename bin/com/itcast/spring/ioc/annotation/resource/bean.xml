<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">
	<context:annotation-config />
	<bean id="userDao" class="com.itcast.spring.ioc.annotation.resource.UserDaoImpl">
		<property name="daoID" value="1"/>
	</bean>
	<bean id="userDao2" class="com.itcast.spring.ioc.annotation.resource.UserDaoImpl">
		<property name="daoID" value="2"/>
	</bean>
	<bean id="userService" class="com.itcast.spring.ioc.annotation.resource.UserService"/>
	
</beans>